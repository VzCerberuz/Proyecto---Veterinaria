@model IEnumerable<Proyecto_Veterinaria.Models.RegistroHorarios>

@{
    ViewBag.Title = "Index";
}

<h2>Registro de Horarios a Empleados</h2>

<p>
    @Html.ActionLink("Nueva", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HorarioDefi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HorasExtras)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HorarioDefi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HorasExtras)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
