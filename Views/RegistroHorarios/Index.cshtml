        <th>
            @Html.DisplayNameFor(model => model.tblDatosHorariosId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HorasExtras)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
