        <td>
            @Html.DisplayFor(modelItem => item.tblEmpleadosId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblDatosHorariosId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha2)
        </td>
        <td>
